{
  "name": "@reasonote/lib-sdk-apollo-client-react",
  "version": "0.0.0",
  "private": true,
  "main": "src/",
  "directories": {
    "src": "src",
    "test": "__tests__"
  },
  "scripts": {
    "libbuild": "tsc && tsc --build tsconfig.esm.json",
    "old-gql-codegen": "(yarn gql-codegen:_rsn:build || true) && echo 'running rsn' && yarn gql-codegen:_rsn",
    "old-gql-codegen:watch": "concurrently -n='rsn' \"(yarn gql-codegen:_rsn:build || true) && yarn gql-codegen:_rsn:watch\"",
    "old-gql-codegen:_rsn": "graphql-codegen -c scripts/rsn-codegen-config.ts",
    "old-gql-codegen:_rsn:watch": "yarn gql-codegen:_rsn:_watch",
    "old-gql-codegen:_rsn:_watch": "graphql-codegen --watch -c scripts/rsn-codegen-config.ts",
    "old-gql-codegen:_rsn:dev": "concurrently -n='tsc,codegen' \"yarn gql-codegen:_rsn:build --watch\" \"yarn gql-codegen:_rsn:_watch\"",
    "old-gql-codegen:_rsn:build": "tsc scripts/rsn-codegen-plugin/*.ts --outDir scripts/rsn-codegen-plugin/build/",
    "gql-codegen": "bun run scripts/run.ts",
    "gql-codegen:watch": "bun run --watch scripts/run.ts"
  },
  "dependencies": {
    "@apollo/client": "3.9.9",
    "@babel/core": "^7.13.8",
    "@graphql-tools/load": "^7.8.9",
    "@graphql-tools/merge": "^8.3.15",
    "async-mutex": "^0.3.2",
    "babel-loader": "^8.2.2",
    "dataloader": "^2.1.0",
    "graphql": "^16.6.0",
    "graphql-tag": "^2.12.6",
    "graphql.macro": "^1.4.2",
    "lodash": "^4.17.21",
    "luxon": "^3.2.1",
    "react-infinite-scroll-component": "^6.1.0",
    "uuid": "^8.3.2",
    "zod": "^3.23.8",
    "@reasonote/lib-sdk-apollo-client": "*"
  },
  "peerDependencies": {
    "react": "^17.0.2 || ^18.0.0",
    "react-dom": "^17.0.2 || ^18.0.0"
  },
  "devDependencies": {
    "@commander-js/extra-typings": "^9.4.1",
    "@graphql-codegen/cli": "^2.16.1",
    "@graphql-codegen/client-preset": "1.2.6",
    "@graphql-codegen/plugin-helpers": "3.1.2",
    "@graphql-codegen/typescript": "^2.5.1",
    "@graphql-codegen/typescript-generic-sdk": "^2.3.12",
    "@graphql-codegen/typescript-graphql-request": "^4.4.10",
    "@graphql-codegen/typescript-operations": "^2.4.2",
    "@graphql-codegen/typescript-react-apollo": "3.3.7",
    "@graphql-tools/load-files": "^6.6.1",
    "@graphql-typed-document-node/core": "^3.1.1",
    "@types/commander": "2.12.2",
    "@types/jest": "^27.0.1",
    "@types/lodash": "^4.14.168",
    "@types/node": "^17.0.21",
    "@types/uuid": "^8.3.0",
    "@types/xml2js": "^0.4.9",
    "babel-plugin-macros": "^3.1.0",
    "commander": "^8.2.0",
    "concurrently": "7.6.0",
    "dotenv-cli": "^4.0.0",
    "jest": "^27.1.0",
    "jest-junit": "^13.0.0",
    "ts-jest": "^27.0.5",
    "ts-node-dev": "^1.1.8",
    "ts-to-zod": "^1.11.0",
    "typescript": "^4.1.3",
    "bun": "1.1.21"
  },
  "jest-junit": {
    "suiteName": "Services Integration Tests",
    "outputDirectory": "./src/__tests__/test-results/",
    "outputName": "results-junit.xml",
    "classNameTemplate": "{classname}-{title}",
    "titleTemplate": "{classname}-{title}",
    "ancestorSeparator": " â€º ",
    "usePathForSuiteName": "true"
  }
}
