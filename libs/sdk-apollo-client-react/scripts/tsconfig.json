
{  
    "compilerOptions": {
        "target": "es5",
        "lib": [
          "esnext"
        ],
        "allowJs": true,
        "skipLibCheck": true,
        "esModuleInterop": true,
        "allowSyntheticDefaultImports": true,
        "strict": true,
        "forceConsistentCasingInFileNames": true,
        "module": "CommonJS",
        "moduleResolution": "node",
        "resolveJsonModule": true,
        "isolatedModules": true,
        "noEmit": true,
        "types": []
      },
      "include": [
          "."
      ],
      "exclude": [
          "node_modules",
          "../../node_modules/*",
          "../../../node_modules",
          "../node_modules",
      ]
    }