{
  "name": "Reasonote",
  "version": "1.0.0",
  "private": true,
  "workspaces": {
    "packages": [
      "apps/*",
      "libs/*"
    ],
    "nohoist": [
      "**/html-minifier-terser",
      "**/babel-jest",
      "**/babel-loader",
      "**/webpack",
      "**/jest/**",
      "**/jest",
      "**/ts-jest/**",
      "**/ts-jest",
      "**/@types/jest",
      "**/@types/jest/**",
      "**/@types/react-native/**",
      "**/react-native/**",
      "**/react-native*",
      "**/@types/react-native*",
      "**/@types/react-native/**"
    ]
  },
  "description": "Scripts to Run the Reasonote Monorepo",
  "main": "index.js",
  "repository": "https://github.com/reasonote/Reasonote.git",
  "author": "marviel <lukebechtel4@gmail.com>",
  "license": "MIT",
  "scripts": {
    "dev": "concurrently -n sb,next-main,lib-ai,docs,vector-worker,lmnr 'yarn sb:restart' 'yarn next:dev' 'yarn lib-ai:build:watch' 'yarn dev:docs' 'yarn dev:vector-worker' 'yarn rsn lmnr:up'",
    "dev:log": "./dev-with-log.sh",
    "dev:docs": "yarn dotenv yarn workspace @reasonote/app-dev-docs dev",
    "dev:vector-worker": "yarn dotenv -- -- sh -c 'cd bun_apps/vector-worker && bun dev'",
    "dev:lmnr": "yarn rsn lmnr:up",
    "build": "turbo build",
    "clean": "turbo clean",
    "clean:node_modules": "find . -name 'node_modules' -type d -prune -print0 | xargs -0 rm -rf",
    "clean:node_modules:quick": "rm -rf node_modules && rm -rf ./apps/*/node_modules && rm -rf ./libs/*/node_modules", 
    "type-check": "turbo type-check --",
    "lint": "turbo lint --",
    "format": "turbo format",
    "test": "yarn test:turbo",
    "test:unit": "dotenv turbo test:unit -- --force --",
    "test:api": "dotenv turbo test:api -- --force --",
    "test:unit:watch": "dotenv turbo test:unit:watch -- --force --",
    "test:api:watch": "dotenv turbo test:api:watch -- --force --",
    "test:vitest": "yarn dotenv vitest run -- -- --dir tests/vitest",
    "test:db": "yarn dotenv vitest run -- -- --dir tests/vitest",
    "test:graph-srs": "yarn dotenv yarn workspace @reasonote/graph-srs test -- --",
    "test:turbo": "dotenv turbo test",
    "test:playwright": "yarn dotenv playwright test -- -- --workers=20",
    "test:e2e": "yarn test:playwright",
    "test:fe": "yarn dotenv yarn workspace @reasonote/app-next-main test -- --",
    "test:fe:vitest": "yarn dotenv yarn workspace @reasonote/app-next-main test:vitest -- ",
    "test:aigen": "yarn dotenv yarn workspace @reasonote/ai-generators test -- --",
    "test:aigen:watch": "yarn dotenv yarn workspace @reasonote/ai-generators test:watch -- --",
    "test:utils": "yarn dotenv yarn workspace @reasonote/lib-utils test -- --",
    "test:ai": "yarn dotenv yarn workspace @reasonote/lib-ai test -- --",
    "test:ai:eval": "yarn dotenv yarn workspace @reasonote/lib-ai test:eval -- --",
    "format:check": "turbo format:check",
    "git:modify-commit-msg": "yarn dotenv yarn ts-node --project scripts/tsconfig.json scripts/git/modifyCommitMsg",
    "test:live": "yarn dotenv yarn workspace @reasonote/app-next-main test --",
    "build:clean": "yarn gen:clean && yarn build",
    "build:check": "yarn build:clean && yarn clean",
    "build:watch": "nodemon --watch apps --watch libs --exec 'yarn build'",
    "next:dev": "yarn dotenv yarn workspace @reasonote/app-next-main dev",
    "gen:clean": "yarn supabase:db:regen-schema -r && yarn sdk:gen:types:rest && yarn gql-codegen:run",
    "gen": "yarn gql-codegen:run && yarn gql-react:run && yarn sdk:gen:types:rest",
    "gen:watch": "concurrently -n ac,ac-react 'yarn gql-codegen:watch' 'yarn gql-react:watch'",
    "gen:db": "yarn supabase:db:regen-schema -r --selectedSchemas public",
    "gen:rest": "yarn sdk:gen:types:rest",
    "gen:gql": "yarn dotenv yarn workspace @reasonote/lib-sdk-apollo-client gql-codegen && yarn gql-react:run",
    "sb": "yarn supabase",
    "db:dump": "pg_dump --column-inserts --data-only --schema public postgresql://postgres:postgres@localhost:65433/postgres > dump.sql",
    "rsn": "yarn dotenv ts-node --project scripts/tsconfig.json scripts/rsn -- --",
    "sb:restart": "yarn sb stop && yarn sb start",
    "native:dev": "yarn dotenv yarn workspace @reasonote/app-native dev",
    "sdk:gen:types:rest": "yarn --silent supabase gen types typescript --local > libs/lib-sdk/src/services/supabase/supabase-rest/codegen/supabase-rest-codegen.ts",
    "supabase:db:tail-logs": "docker exec -it supabase_db_Reasonote bash -c 'cat /var/lib/postgresql/data/current_logfiles | awk \"{print $2}\" | xargs tail -f'",
    "supabase:db:regen-schema": "ts-node-dev --project scripts/tsconfig.json scripts/supabase/db/regen-schema",
    "supabase:db:check-types": "ts-node-dev --project scripts/tsconfig.json scripts/supabase/db/check-types",
    "supabase:db:format-json-to-tuples": "ts-node-dev --project scripts/tsconfig.json scripts/supabase/db/format-json-to-tuples",
    "supabase:db:schedule-cron-jobs": "ts-node-dev --project scripts/tsconfig.json scripts/supabase/db/schedule-cron-jobs",
    "supabase:db:cli-migration": "cd supabase/data_migration && yarn cli-migration",
    "gql-codegen:watch": "yarn dotenv yarn workspace @reasonote/lib-sdk-apollo-client gql-codegen:watch",
    "gql-react:watch": "yarn dotenv yarn workspace @reasonote/lib-sdk-apollo-client-react gql-codegen:watch",
    "gql-react:run": "yarn dotenv yarn workspace @reasonote/lib-sdk-apollo-client-react gql-codegen",
    "gql-codegen:run": "yarn dotenv yarn workspace @reasonote/lib-sdk-apollo-client gql-codegen && yarn gql-react:run",
    "script:simple-notion-agent": "yarn dotenv yarn workspace @reasonote/app-scripts simple-notion-agent",
    "script:test-hn-loader": "yarn dotenv yarn workspace @reasonote/app-scripts test-hn-loader"
  },
  "dependencies": {
    "@lukebechtel/lab-ts-utils": "6.4.0",
    "@supabase/supabase-js": "^2.45.0",
    "dotenv": "^16.0.3",
    "lodash": "^4.17.21",
    "pg": "^8.7.3"
  },
  "devDependencies": {
    "@commander-js/extra-typings": "11.0.0",
    "@playwright/browser-chromium": "^1.39.0",
    "@playwright/test": "^1.39.0",
    "@types/commander": "^2.12.2",
    "@types/lodash": "^4.14.199",
    "@types/node": "^18.0.0",
    "@types/pg": "^8.6.5",
    "@typescript-eslint/eslint-plugin": "^5.58.0",
    "@typescript-eslint/parser": "^5.58.0",
    "commander": "11.0.0",
    "concurrently": "7.6.0",
    "dotenv-cli": "^6.0.0",
    "eslint": "^8.38.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-standard-with-typescript": "34.0.1",
    "eslint-plugin-react": "^7.32.2",
    "nodemon": "2.0.21",
    "openai": "4.56.1",
    "vitest": "2.1.8",
    "ts-jest": "^27.0.5",
    "supabase": "2.20.5",
    "ts-node": "^10.9.1",
    "@vitest/browser": "2.1.8",
    "ts-node-dev": "^2.0.0",
    "turbo": "^1.10.16",
    "typescript": "^4.7.4",
    "prompts": "^2.4.2",
    "@types/prompts": "^2.4.9"
  },
  "resolutions": {
    "unix-dgram": "{}"
  }
}
